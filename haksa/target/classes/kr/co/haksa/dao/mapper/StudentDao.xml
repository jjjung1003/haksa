<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybats-3-mapper.dtd">
<mapper namespace="kr.co.haksa.dao.StudentDao">	<!-- 사용할 인터페이스 -->

	<insert id="write_ok" parameterType="kr.co.haksa.dto.StudentDto">
		insert into student(hakbun,name,zip,juso,juso_etc,hphone,birth,writeday)
		values(#{hakbun},#{name},#{zip},#{juso},#{juso_etc},#{hphone},#{birth},now())
	</insert>

	<select id="list" resultType="kr.co.haksa.dto.StudentDto">
		select * from student
	</select>
















</mapper>